<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!--
  user_password.xml Configuration -
  This file contains the userid and password that will be used to 
  	a) Obtain a valid Kerboros ticket (if required). and/or
  	b) Log-on to cluster via JDBC.
  	
  	It is assumed this file is protected and kept in user's home directory.
  	
-->
<configuration>
 
  <property>
    <name>jdbc.username</name>
    <value>vagrant</value>
    <description>
      Username.
    </description>
  </property>

  <property>
    <name>jdbc.password</name>
    <value></value>
    <description>
      Password.
    </description>
  </property>

</configuration>  
